<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!--Pen Sizes-->
    <add key="Pen.Size.Default" value="2"/>
    <add key="Pen.Size.Small" value="2"/>
    <add key="Pen.Size.Medium" value="6"/>
    <add key="Pen.Size.Large" value="9"/>
    <add key="Eraser.Size" value="15"/>
    
    <!--Images Path-->
    <add key="Eraser.Cursor.Path" value=".\\Images\\Eraser.cur"/>
    <add key="Button.Main" value="Images/button-main.png"/>
    <add key="Button.Eraser" value="Images/button-eraser.png"/>
    <add key="Button.PenSize" value="Images/button-pensize.png"/>
    <add key="Button.PenSize.S" value="Images/button-pensize-s.png"/>
    <add key="Button.PenSize.M" value="Images/button-pensize-m.png"/>
    <add key="Button.PenSize.L" value="Images/button-pensize-l.png"/>
    <add key="Button.Color" value="Images/button-color.png"/>
    <add key="Button.Action" value="Images/button-action.png"/>
    <add key="Button.Action.Undo" value="Images/button-action-undo.png"/>
    <add key="Button.Action.Redo" value="Images/button-action-redo.png"/>
    <add key="Button.Save" value="Images/button-save.png"/>
    <add key="Button.Coming.Soon" value="Images/button-coming-soon.png"/>
  </appSettings>
</configuration>